<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://www.jooq.org/xsd/jooq-codegen-3.18.0.xsd">
    <!-- Specify the database connection settings -->
    <jdbc>
        <driver>com.mysql.cj.jdbc.Driver</driver>
        <url>jdbc:mysql://localhost:3306/minecraft</url>
        <user>minecraft</user>
        <password></password>
    </jdbc>

    <!-- Specify the code generation settings -->
    <generator>
        <name>org.jooq.codegen.DefaultGenerator</name>
        <database>
            <name>org.jooq.meta.mysql.MySQLDatabase</name>
            <includes>.*</includes>
            <excludes></excludes>
            <inputSchema>minecraft</inputSchema>
        </database>
        <target>
            <packageName>ca.ultracrepidarianism.kingdom.model.test</packageName>
            <directory>src/main/java</directory>
        </target>
    </generator>
</configuration>